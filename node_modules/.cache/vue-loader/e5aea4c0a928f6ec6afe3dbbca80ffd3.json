AAC,CAAC,CAAC,CAAC,CAAC,CAAC;AACrB","file":"/home/hafiz/Development/Self Projects/Vue Projects/vuex-modules/src/components/ProductList.vue","sourceRoot":"","sourcesContent":["<template>\n  <ul>\n    <li v-for=\"product in products\" :key=\"product.id\">\n      {{ product.title }} - {{ currency(product.price) }}\n      <!-- <br /> -->\n      <button :disabled=\"!product.inventory\" @click=\"addProductToCart(product)\">\n        Add to cart\n      </button>\n      <!-- <button>Add to Cart</button> -->\n    </li>\n  </ul>\n</template>\n\n<script>\n// import { mapState, mapActions } from \"vuex\";\n// export default {\n//   computed: {\n//     ...mapState([\"products\"]),\n//   },\n//   methods: {\n//     ...mapActions([\"addProductToCart\"]),\n//   },\n//   created() {\n//     this.$store.dispatch(\"getAllProducts\");\n//   },\n// };\n\nimport { useStore } from \"vuex\";\nimport { computed } from \"vue\";\nimport { currency } from \"../currency\";\nexport default {\n  setup() {\n    const store = useStore();\n\n    const products = computed(() => store.state.products.products);\n    const addProductToCart = (product) =>\n      store.dispatch(\"products/addProductToCart\", product);\n\n    store.dispatch(\"products/getAllProducts\");\n\n    return {\n      store,\n      addProductToCart,\n      products,\n    };\n  },\n};\n</script>\n\n<style scoped>\nul {\n  list-style: none;\n}\nli {\n  display: flex;\n  margin: 10px auto;\n  justify-content: space-between;\n  align-items: center;\n}\n</style>\n"]}]}